     return getAttributesMapping ( attributesJsonObject , fieldMapArray );
     ---------------------------------------------
       => this has two methods :1. getSimpleAttributesMapping ( attributesJsonObject )
                                2. getNestedAttributesMapping ( fieldMapArray )
       
       public static List< CommonAttributeMapping > getAttributesMapping ( JSONObject attributesJsonObject , JSONArray fieldMapArray ) {
       
        return ListUtils.union ( getSimpleAttributesMapping ( attributesJsonObject ) , getNestedAttributesMapping ( fieldMapArray ) );
        
    }
 
    ---------------------------------------------------
    
    => this method gets the json object commonattributes mapping from RDP 
    => and parses through each object and created an commonattributeMapping. there are 181 commonattributes
    => all this are adding into the commonAttributeMappingList which are usind in commonattributes list cretaion class:
    => 
    
     private static List< CommonAttributeMapping > getSimpleAttributesMapping ( JSONObject attributesJsonObject ) {
     
        List < CommonAttributeMapping > commonAttributeMappingList = new ArrayList <> ( );
        
        attributesJsonObject.keySet ( ).forEach ( key -> {  => gets one objecct from json obejct received from rdp
        
           ex: "piesproductcategorycode": {"values":[{"id":"53fdea49-2a3a-48ef-86da-f187232a5f52","source":"internal","locale":"en-US","value":"AAIAProductCategoryCode"}],"properties":{"isAttribute":true,"id":1,"required":true,"enabled":true}}
           
           ex:"piesnationalpopularitycode": {"values":[{"id":"bea0bc09-5552-49ff-99d0-b158c2e46e86","locale":"en-US","value":"ExtendedInformation/ExtendedProductInformation[@EXPICode='NPC']"}],"properties":{"isAttribute":true,"externalAction":"@actionAttr(ExtendedProductInformation[@EXPICode='NPC']/@MaintenanceType)","id":1,"required":true,"enabled":true}}
         
            CommonAttributeMapping commonAttributeMapping = new CommonAttributeMapping ( );
            
                => now we create an object of commonattributemappingclass which has multiple methods 
                => in the  we create more than 100 objects of  commonAttributeMapping and place them in list
            
            commonAttributeMapping.setKey ( key );
                 => we are setting key in that object of commonAttributeMapping
            
            commonAttributeMapping.setNested ( false );
            
            JSONObject attributeJsonObject = attributesJsonObject.getJSONObject ( key );
                  => now we get the object from attributemapping list we got from rdp with the help of key:
                    ex: 
                     {"values":[{"id":"f072f769-a4bf-465b-b609-32cbe555e292","locale":"en-US","value":"AAIAProductCategoryCode"}],"properties":{"isAttribute":true,"id":1,"required":true,"enabled":true}}
                     
                     ex: {"values":[{"id":"bea0bc09-5552-49ff-99d0-b158c2e46e86","locale":"en-US","value":"ExtendedInformation/ExtendedProductInformation[@EXPICode='NPC']"}],"properties":{"isAttribute":true,"externalAction":"@actionAttr(ExtendedProductInformation[@EXPICode='NPC']/@MaintenanceType)","id":1,"required":true,"enabled":true}}
            
            JSONArray attributeJsonArray = attributeJsonObject.getJSONArray ( "values" );
                    ex: [{"id":"f072f769-a4bf-465b-b609-32cbe555e292","locale":"en-US","value":"AAIAProductCategoryCode"}]
                    
                    ex: [{"id":"bea0bc09-5552-49ff-99d0-b158c2e46e86","locale":"en-US","value":"ExtendedInformation/ExtendedProductInformation[@EXPICode='NPC']"}]
                    
                    
            for (Object object : attributeJsonArray) {
                JSONObject jsonObject = (JSONObject) object;
                String path = jsonObject.get ( "value" ).toString ( );
                
                 => AAIAProductCategoryCode ex: ExtendedInformation/ExtendedProductInformation[@EXPICode='NPC']
                 
                if (!path.contains ( "[" )) { 
                    pathContainsBracket(commonAttributeMapping, path);

                } else {
                    pathContainsNoBracket (commonAttributeMapping, path  );
                }
            }
            commonAttributeMappingList.add ( commonAttributeMapping );
        } );
        return commonAttributeMappingList;
    }
                            ------------------------------------------
                            
                            
     private static void  pathContainsBracket(CommonAttributeMapping commonAttributeMapping, String path){
     
        commonAttributeMapping.setConditional ( false );
        
        List < String > pathList = new ArrayList <> ( );
        
        boolean appendContent = false;
        
        String[] pathElements = path.split ( "/" );
        
        for (String element : pathElements) {
            if (element.startsWith ( "@" )) {
                appendContent = false;
                pathList.add ( element.replace ( "@" , "" ) );
            } else {
                appendContent = true;
                pathList.add ( element );
            }
        }
        if (appendContent) {
            pathList.add ( CONTENT );
        }
        commonAttributeMapping.setPath ( pathList );

    }
  
 
 
 
 
