    public static List < CommonAttributeMapping > getNestedAttributesMapping ( JSONArray fieldMapArray ) {
    
        List < CommonAttributeMapping > commonAttributeMappingList = new ArrayList <> ( );
        
        for (Object object : fieldMapArray) {
        
         ex: {"collectionType":false,"childFieldMappings":[{"uom":"Price/@UOM","entityType":"vendoritem","destination":"@nestedAttrExp(piesprices.group[%d].piesprice)","id":2004655947,"source":"Price","locale":"en-US","hasUOM":true,"required":true,"enabled":true},{"entityType":"vendoritem","destination":"@nestedAttrExp(piesprices.group[%d].piesexpirationdate)","id":2004655948,"source":"ExpirationDate","locale":"en-US","hasUOM":false,"required":true,"enabled":true},{"entityType":"vendoritem","destination":"@nestedAttrExp(piesprices.group[%d].piespricesheetleveleffectivedate)","id":2004655949,"source":"EffectiveDate","locale":"en-US","hasUOM":false,"required":true,"enabled":true},{"entityType":"vendoritem","destination":"@nestedAttrExp(piesprices.group[%d].piespricecurrencycode)","id":2004655950,"source":"CurrencyCode","locale":"en-US","hasUOM":false,"required":true,"enabled":true},{"entityType":"vendoritem","destination":"@nestedAttrExp(piesprices.group[%d].piespricepricesheetnumber)","id":2004655951,"source":"PriceSheetNumber","locale":"en-US","hasUOM":false,"required":true,"enabled":true},{"entityType":"vendoritem","destination":"@nestedAttrExp(piesprices.group[%d].piespricetype)","id":2004655952,"source":"@PriceType","locale":"en-US","hasUOM":false,"required":true,"enabled":true},{"entityType":"vendoritem","destination":"@nestedAttrExp(piesprices.group[%d].piespricetypedescription)","id":2004655953,"source":"PriceTypeDescription","locale":"en-US","hasUOM":false,"required":true,"enabled":true},{"entityType":"vendoritem","destination":"@nestedAttrExp(piesprices.group[%d].piespriceuom)","id":2004655954,"source":"Price/@UOM","locale":"en-US","hasUOM":false,"required":true,"enabled":true},{"uom":"PriceBreak/@UOM","entityType":"vendoritem","destination":"@nestedAttrExp(piesprices.group[%d].piespricebreakquantity)","id":2004655955,"source":"PriceBreak","locale":"en-US","hasUOM":true,"required":true,"enabled":true},{"entityType":"vendoritem","destination":"@nestedAttrExp(piesprices.group[%d].piespricebreakquantityuom)","id":2004655955,"source":"PriceBreak/@UOM","locale":"en-US","hasUOM":false,"required":true,"enabled":true}],"entityType":"vendoritem","destination":"piesprices","id":1516043207,"source":"Prices/Pricing","type":"nested","required":true,"enabled":true}
        
            JSONObject fieldMappingObject = (JSONObject) object;
            
            if (fieldMappingObject.has ( "type" ) && fieldMappingObject.getString ( "type" ).equalsIgnoreCase ( "nested" )) {
              => checks the object consist of key "type" if yes then goes in
            
            
                if (!fieldMappingObject.getBoolean ( "collectionType" )) {
                  => checks if object consist of key "collectionType"  and its value as false:\
                  
                    CommonAttributeMapping commonAttributeMapping = new CommonAttributeMapping ( );
                    
                    commonAttributeMapping.setNested ( true );
                    
                    commonAttributeMapping.setKey ( fieldMappingObject.getString ( DESTINATION ) );
                     gets this from object  "destination": "piesprices", and sets it in commonattributemapping object
                     
                    commonAttributeMapping.setHasKeySuffixPath ( false );
                    
                    commonAttributeMapping.setPath ( Arrays.asList ( fieldMappingObject.getString ( SOURCE ).split ( "/" ) ) );
                                "source": "Prices/Pricing"  sets list of two
                                
                    List < Object > groupElementsList = new ArrayList <> ( );
                    
                     ** this list os used to add the commonattributemappings object of each chieldfieldmappings:
                    
                    JSONArray childFieldMappingsArray = fieldMappingObject.getJSONArray ( "childFieldMappings" );
                    
            "childFieldMappings" :        [{"uom":"Price/@UOM","entityType":"vendoritem","destination":"@nestedAttrExp(piesprices.group[%d].piesprice)","id":2004655947,"source":"Price","locale":"en-US","hasUOM":true,"required":true,"enabled":true},{"entityType":"vendoritem","destination":"@nestedAttrExp(piesprices.group[%d].piesexpirationdate)","id":2004655948,"source":"ExpirationDate","locale":"en-US","hasUOM":false,"required":true,"enabled":true},{"entityType":"vendoritem","destination":"@nestedAttrExp(piesprices.group[%d].piespricesheetleveleffectivedate)","id":2004655949,"source":"EffectiveDate","locale":"en-US","hasUOM":false,"required":true,"enabled":true},{"entityType":"vendoritem","destination":"@nestedAttrExp(piesprices.group[%d].piespricecurrencycode)","id":2004655950,"source":"CurrencyCode","locale":"en-US","hasUOM":false,"required":true,"enabled":true},{"entityType":"vendoritem","destination":"@nestedAttrExp(piesprices.group[%d].piespricepricesheetnumber)","id":2004655951,"source":"PriceSheetNumber","locale":"en-US","hasUOM":false,"required":true,"enabled":true},{"entityType":"vendoritem","destination":"@nestedAttrExp(piesprices.group[%d].piespricetype)","id":2004655952,"source":"@PriceType","locale":"en-US","hasUOM":false,"required":true,"enabled":true},{"entityType":"vendoritem","destination":"@nestedAttrExp(piesprices.group[%d].piespricetypedescription)","id":2004655953,"source":"PriceTypeDescription","locale":"en-US","hasUOM":false,"required":true,"enabled":true},{"entityType":"vendoritem","destination":"@nestedAttrExp(piesprices.group[%d].piespriceuom)","id":2004655954,"source":"Price/@UOM","locale":"en-US","hasUOM":false,"required":true,"enabled":true},{"uom":"PriceBreak/@UOM","entityType":"vendoritem","destination":"@nestedAttrExp(piesprices.group[%d].piespricebreakquantity)","id":2004655955,"source":"PriceBreak","locale":"en-US","hasUOM":true,"required":true,"enabled":true},{"entityType":"vendoritem","destination":"@nestedAttrExp(piesprices.group[%d].piespricebreakquantityuom)","id":2004655955,"source":"PriceBreak/@UOM","locale":"en-US","hasUOM":false,"required":true,"enabled":true}]
                    
                    
                    for (Object childObject : childFieldMappingsArray) {
                       ex: {"uom":"Price/@UOM","entityType":"vendoritem","destination":"@nestedAttrExp(piesprices.group[%d].piesprice)","id":2004655947,"source":"Price","locale":"en-US","hasUOM":true,"required":true,"enabled":true}
                       
                        JSONObject childJsonObject = (JSONObject) childObject;
                        
                        CommonAttributeMapping elementAttributeMapping = getElementAttributeMapping ( childJsonObject );
                        
                        groupElementsList.add ( elementAttributeMapping );
                    }
                    commonAttributeMapping.setGroupElementsList ( groupElementsList );
                    commonAttributeMappingList.add ( commonAttributeMapping );
                } else {
                    Map < String, List < Object > > collectionMap = new HashMap <> ( );
                    JSONArray childFieldMappingsArray = fieldMappingObject.getJSONArray ( "childFieldMappings" );
                    for (Object childObject : childFieldMappingsArray) {
                        List < Object > groupElementsList;
                        JSONObject childJsonObject = (JSONObject) childObject;
                        CommonAttributeMapping elementAttributeMapping = getElementAttributeMapping ( childJsonObject );
                        String destination = childJsonObject.getString ( DESTINATION );
                        String[] keyData = destination.substring ( destination.indexOf ( "@suffix(" ) + 8 , destination.lastIndexOf ( ");@s" ) ).split ( "," );
                        String collectionMapKey = keyData[1].trim ( ) + "," + keyData[0].trim ( );
                        if (!collectionMap.containsKey ( collectionMapKey )) {
                            groupElementsList = new ArrayList <> ( );
                        } else {
                            groupElementsList = collectionMap.get ( collectionMapKey );
                        }
                        String[] elementKeyData = destination.substring ( destination.lastIndexOf ( "@suffix(" ) + 8 , destination.lastIndexOf ( ")" ) ).split ( "," );
                        elementAttributeMapping.setHasKeySuffixPath ( true );
                        elementAttributeMapping.setKeySuffixPath ( Arrays.asList ( elementKeyData[0] , CONTENT ) );
                        groupElementsList.add ( elementAttributeMapping );
                        collectionMap.put ( collectionMapKey , groupElementsList );
                    }
                    for (Map.Entry < String, List < Object > > item : collectionMap.entrySet ( )) {
                        String[] keyData = item.getKey ( ).split ( "," );
                        CommonAttributeMapping commonAttributeMapping = new CommonAttributeMapping ( );
                        commonAttributeMapping.setNested ( true );
                        commonAttributeMapping.setKey ( keyData[0] );
                        commonAttributeMapping.setHasKeySuffixPath ( true );
                        commonAttributeMapping.setKeySuffixPath ( Arrays.asList ( keyData[1] , CONTENT ) );
                        commonAttributeMapping.setPath ( Arrays.asList ( fieldMappingObject.getString ( SOURCE ).split ( "/" ) ) );
                        commonAttributeMapping.setGroupElementsList ( item.getValue ( ) );
                        commonAttributeMappingList.add ( commonAttributeMapping );
                    }
                }
            }
        }
        return commonAttributeMappingList;
    }

 
 
 
 
 
 
 
 
 
 
 
 
 
 
 
 
 
 
 
 
 
       
