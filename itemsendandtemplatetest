  public static List< CommonAttributeMapping > getAttributesMapping () {
            
     JSONObject attributesJsonObject = new JSONObject (  );
        
     JSONArray fieldMapArray = new JSONArray (  );
     try {
        
      String commonMappingQuery = ModuleConfig.piesConfig.commonMappingquery;
            
             -> gets the mapping query {"params":{"query":{"id":"vendoritem_attributemappings","filters":{"typesCriterion":
               ["attributemapping"],"allContextual":true}},"fields":{"attributes":["_ALL"]}}} from the specified location
            
     attributesJsonObject = (JSONObject) EntityHelpers.queryRDP ( "entityappservice" , "get" , commonMappingQuery , Config.Tenant ).query ( "/response/entities/0/data/contexts/0/attributes" );
            
             -> with the help of rdp rest call it gets the rsjson format for commonattributes
            
     String nestedMappingQuery = ModuleConfig.piesConfig.nestedMappingQuery;
            
              -> gets the nestedMappingQuery from the location:
                   {"params":{"query":{"id":"Xml_Import_Azure_Blob_Container_PIES","contexts":[{"app":"RSConnect"}],"filters":
                      {"typesCriterion":["integrationprofile"]}},"fields":{"attributes":["_ALL"]}}}
            
     fieldMapArray = (JSONArray) EntityHelpers.queryRDP ( "configurationservice" , "get" , nestedMappingQuery , Config.Tenant ).query ( "/response/configObjects/0/data/contexts/0/jsonData/transform/fieldMap" );
            
               ->  -> with the help of rdp rest call it gets the rsjson format for nestedMappingQuery you can find them at end of file
            
  } catch ( Exception ex ) {
            log.error ( "Error  " + ex.fillInStackTrace ( ) );
        }
        return getAttributesMapping ( attributesJsonObject , fieldMapArray );
 }
 
 
 ---------------------------------------------------------------------------------------------------------
 
 => mow we have two objects 1. attributesJsonObject
